<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/persistence" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_2.xsd">
	<persistence-unit name="StartUpElec" transaction-type="JTA">
	<jta-data-source>java:/StartUpElecDS</jta-data-source>
	<class>pe.edu.upc.StartUpElec.models.entity.Customer</class>
	<class>pe.edu.upc.StartUpElec.models.entity.Service</class>
	<class>pe.edu.upc.StartUpElec.models.entity.Typecard</class>
	<class>pe.edu.upc.StartUpElec.models.entity.TypecardHistory</class>
	<exclude-unlisted-classes>false</exclude-unlisted-classes>
	<properties>
	<property name="javax.persistencce.schema-generation.database.action" value="drop-and-create"/>
	
	</properties>
	</persistence-unit>
</persistence>
